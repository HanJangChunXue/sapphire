    <include schemaLocation="http://www.eclipse.org/sapphire/tests/xml/xsd/0002/b" />

    <element name="element">
        <complexType>
            <sequence>
                <element name="aaa" type="string"/>
                <element name="bbb" type="string"/>
                <element name="ccc" type="string"/>
